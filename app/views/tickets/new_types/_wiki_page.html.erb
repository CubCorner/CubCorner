<% @wiki = @ticket.model %>
<h4>You are reporting the following wiki page:</h4>
<div class="page"><%= link_to(@wiki.pretty_title, @wiki) %></div>
<span class="date">Created <%= time_ago_in_words_tagged(@wiki.created_at) %></span>
<br><br>
<div class="dtext-container">
  <%= format_text(@wiki.body) %>
</div>
